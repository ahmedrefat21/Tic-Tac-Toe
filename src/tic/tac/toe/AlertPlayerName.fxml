<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="320.0" styleClass="alert-box" stylesheets="@css/AlertPlayerName.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fill="#362900" layoutX="89.0" layoutY="29.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="head-text" text="Enter Players Name" textAlignment="CENTER" />
      <Text fill="#362900" layoutX="38.0" layoutY="72.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="players-name" text="Player 1" textAlignment="CENTER" />
      <Text fill="#362900" layoutX="38.0" layoutY="126.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="players-name" text="Player 2" textAlignment="CENTER" />
      <TextField fx:id="player_one_name" layoutX="131.0" layoutY="51.0" promptText="player-one" styleClass="players-name-txt-filed" stylesheets="@css/AlertPlayerName.css" />
      <TextField fx:id="player_two_name" layoutX="131.0" layoutY="105.0" promptText="player-two" styleClass="players-name-txt-filed" stylesheets="@css/AlertPlayerName.css" />
      <Button fx:id="Ok_button" layoutX="131.0" layoutY="149.0" onAction="#handleButtonAction" style="-fx-effect: d;" styleClass="ok-button" stylesheets="@css/AlertPlayerName.css" text="OK" />
      <Button fx:id="Cancel_button" layoutX="202.0" layoutY="149.0" mnemonicParsing="false" styleClass="cancel-button" stylesheets="@css/AlertPlayerName.css" text="Cancel" />
   </children>
</AnchorPane>
